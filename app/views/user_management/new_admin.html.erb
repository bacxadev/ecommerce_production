        <script>
          $('.abc').select2();

          $('.abc').on('select2:opening select2:closing', function( event ) {
              var $searchfield = $(this).parent().find('.select2-search__field');
              $searchfield.prop('disabled', true);
          });
        </script>

        <script>
          document.addEventListener('DOMContentLoaded', function() {
            const form = document.querySelector('form');
            const selectedDomainsInput = document.createElement('input');
            selectedDomainsInput.type = 'hidden';
            selectedDomainsInput.name = 'selected_domains'; // Change to the appropriate attribute name
            form.appendChild(selectedDomainsInput);

            $('#selectedDomains').select2(); // Initialize Select2

            form.addEventListener('submit', function() {
              const selectedDomains = $('#selectedDomains').val();
              selectedDomainsInput.value = selectedDomains.join(',');
            });
          });
        </script>
